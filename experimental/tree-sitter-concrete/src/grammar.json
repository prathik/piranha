{
  "name": "concrete",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "template_expression"
        },
        {
          "type": "SYMBOL",
          "name": "token_expression"
        }
      ]
    },
    "template_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":["
        },
        {
          "type": "SYMBOL",
          "name": "template"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "template": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "var_name"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "var_name"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "node_type"
            }
          ]
        }
      ]
    },
    "token_expression": {
      "type": "PATTERN",
      "value": "[^:\\[]+"
    },
    "var_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "node_type": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
